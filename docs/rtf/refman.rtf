00117  */\par
00118 zeroBss:\par
00119     ldr r0, =__bss_start__\par
00120     ldr r1, =__bss_end__\par
00121     movs r2, #0\par
00122     b 2f\par
00123 1:  \par
00124     str r2, [r0]\par
00125     adds r0, #4\par
00126 2:  \par
00127     cmp r0, r1\par
00128     blo 1b\par
00129     bx lr\par
00130 \par
00131     .global copyData\par
00132     .type copyData, %function\par
00133 /**\par
