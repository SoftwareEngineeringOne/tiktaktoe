00138  * from the load address in flash to the execution address in RAM.\par
00139  */\par
00140 copyData:\par
00141     ldr r0, =__data_start__\par
00142     ldr r1, =__data_end__\par
00143     ldr r2, =__data_load_addr__\par
00144     cmp r0, r1\par
00145     bhs 2f\par
00146 1:\par
00147     ldr r3, [r2]\par
00148     str r3, [r0]\par
00149     adds r0, #4\par
00150     adds r2, #4\par
00151     cmp r0, r1\par
00152     blo 1b\par
00153 2:\par
00154     bx lr\par
00155 \par
