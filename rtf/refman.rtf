00228 {\cf17 static} {\cf18 void} calculateSummary()\par
00229 \{\par
00230     game_state.total_ticks = g_timer.ticks_total;\par
00231 \par
00232     {\cf19 if}(game_state.mode == PVE)\par
00233     \{\par
00234         game_state.average_ticks = g_timer.ticks_total / 2 / game_state.round;\par
00235     \}\par
00236     {\cf19 else}\par
